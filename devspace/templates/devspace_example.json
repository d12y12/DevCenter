{
  "version": "1.0",
  "maintainer": "",
  "project": {
    "name": "",
    "path": ""
  },
  "servers": {
    "Web": {
      "host": "",
      "port": 8888,
      "type": "alpine",
      "localization": true
    },
    "GitMirror": {
      "type": "alpine",
      "localization": true
    },
    "DocBuilder": {
      "type": "alpine",
      "localization": true,
      "builder": [
        "docbook",
        "sphinx"
      ]
    }
  },
  "services": {
    "yocto": {
      "Web": {
        "cgit_options": {
          "title": "Mirror Repositories",
          "description": "",
          "max-repo-count": 50,
          "logo": {
            "light": "${template_dir}/example/yocto-logo-light.png",
            "dark": "${template_dir}/example/yocto-logo-dark.png"
          }
        }
      },
      "GitMirror": {
        "cgit": [
          {
            "source": "http://git.yoctoproject.org/cgit.cgi/crops/",
            "excludes": [
              "http://git.yoctoproject.org/cgit.cgi/yocto-testresults/",
              "http://git.yoctoproject.org/cgit.cgi/yp-qa-build-perf-data/"
            ],
            "targets": []
          }
        ],
        "github": [],
        "synchronization": {
          "consistency": true,
          "crontab": "*/5 * * * *"
        }
      }
    },
    "github": {
      "Web": {
        "cgit_options": {
          "title": "Mirror Repositories",
          "description": "",
          "max-repo-count": 50,
          "logo": {
            "light": "${template_dir}/example/github-logo-light.png",
            "dark": "${template_dir}/example/github-logo-dark.png"
          }
        }
      },
      "GitMirror": {
        "cgit": [],
        "github": [
          {
            "source": "d12y12/temp",
            "excludes": [],
            "targets": []
          },
          {
            "source": "scrapy/scrapy",
            "excludes": [],
            "targets": []
          }
        ]
      },
      "Synchronization": {
        "consistency": true,
        "crontab": "*/5 * * * *"
      }
    },
    "note": {
      "Web": {
      },
      "DocBuilder": {
        "builder": "sphinx",
        "source": "https://github.com/d12y12/notes.git",
        "output": "./_build"
      },
      "Synchronization": {
        "consistency": true,
        "crontab": "*/5 * * * *"
      }
    },
    "yocto-docs": {
      "Web": {
        "autoindex": true
      },
      "DocBuilder": {
        "builder": "docbook",
        "source": "Z:/yocto-docs",
        "output": "./build"
      },
      "Synchronization": {
        "consistency": true,
        "crontab": "*/5 * * * *"
      }
    },
    "test": {
      "Web": {},
      "DocBuilder": {
        "builder": "",
        "source": "",
        "output": ""
      }
    }
  }
}
